<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SefaPortfolyo</title>
    @RenderSection("style",required: false)
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SefaPortfolyo.styles.css" asp-append-version="true" />

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">


    <link rel="stylesheet" href="link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

</head>
<body>
    <header>
        <div class="container" id="navv">
            <nav class="navbar navbar-expand-sm mb-3">
                <div class="container">
                    <a asp-controller="Home" asp-action="Index" class="navbar-brand">AKSU</a>
                    <div class="collaspe navbar-collaspe">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a asp-controller="Home" asp-action="Index" class="nav-link" style="color: black;">ANASAYFA</a>
                            </li>
                            <li class="nav-item">
                                <a asp-controller="Home" asp-action="Hakkimda" class="nav-link" style="color: black;">HAKKIMDA</a>
                            </li>
                            <li class="nav-item">
                                <a asp-controller="Home" asp-action="Projelerim" class="nav-link" style="color: black;">PROJELERİM</a>
                            </li>
                            <li class="nav-item">
                                <a asp-controller="Home" asp-action="Iletisim" class="nav-link" style="color: black;" >İLETİŞİM</a>
                            </li>
                            
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
        
    </header>


    <div class="container" id="container">
        <main role="main" class="pb-3">
            <div class="container mt-3">
                <div class="row">
                    <div class="col-lg-8">
                        <div class="imagem">
                            <img src="~/img/sefaresim.png" alt="">
                        </div>
                        
                    </div>
                    <div class="col-lg-4" style="margin-top: 180px;">
                        @RenderBody()
                    </div>
                </div>
            </div> 
        </main>
    </div>
    
    @RenderSection("ikinicikisim",required: false)


    <footer class="border-top footer text-muted mt-5">
        <div class="container">
            &copy; 2024 - CelalSafaAksuy
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)

    

</body>
</html>
